@model SözlükForum.Models.Kullanici
@{
}

<form method="post">
    @Html.LabelFor(x => x.email)
    @Html.TextBoxFor(x => x.email, new { @class = "form-control " })
    @Html.LabelFor(x => x.sifre)
    @Html.TextBoxFor(x => x.sifre, new { @class = "form-control " })
    @Html.LabelFor(x => x.kullanıcıAd)
    @Html.TextBoxFor(x => x.kullanıcıAd, new { @class = "form-control " })
    <br />
    <button type="submit" class="btn btn-primary btn-block">Gönder</button>
</form>
